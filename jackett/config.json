{
    "name": "Jackett",
    "version": "dev",
    "slug": "jackett",
    "description": "Jackett for Home Assistant",
    "url": "https://github.com/haberda/jackett-addon",
    "startup": "services",
    "webui": "http://[HOST]:[PORT:9117]",
    "panel_icon": "mdi:progress-download",
    "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
    "map": [
      "config:rw",
      "media:rw",
      "share:rw",
      "ssl"
    ],
    "boot": "auto",
    "ports": {
      "9117/tcp": 9117
    },
   "ports_description": {
      "9117/tcp": "Web UI port"
    },
    "privileged": [
      "NET_ADMIN"
    ],
    "devices": [
      "/dev/net/tun"
    ],
    "hassio_api": true,
    "homeassistant_api": false,
    "host_network": false,
    "options": {
      "log_level": "info",
      "openvpn_enabled": false,
      "openvpn_config": "",
      "openvpn_username": "",
      "openvpn_password": ""
    },
    "schema": {
      "log_level": "match(^(trace|debug|info|notice|warning|error|fatal)$)",
      "openvpn_enabled": "bool",
      "openvpn_config": "str",
      "openvpn_username": "str",
      "openvpn_password": "str"
    },
    "environment": {
      "LOG_FORMAT": "{LEVEL}: {MESSAGE}"
    }
    "panel_icon": "mdi:coat-rack",
    "snapshot_exclude": ["*.txt"],
    "image": "eightiesguy/jackett-addon"
  }
